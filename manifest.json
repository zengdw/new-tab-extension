{
  "manifest_version": 3,
  "name": "自定义新标签页",
  "version": "1.0",
  "description": "美观可自定义的新标签页，支持多搜索引擎和常用网站管理",
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 